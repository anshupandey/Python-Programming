    "import paho.mqtt.client as mq\n",
    "import time\n",
    "c = mq.Client()\n",
    "c.connect(\"broker.hivemq.com\")\n",
    "time.sleep(1)\n",
    "c.publish(\"mytopic/today\",\"Hello from Anshu\",retain=True)\n",
    "time.sleep(1)\n",
    "c.disconnect()"
